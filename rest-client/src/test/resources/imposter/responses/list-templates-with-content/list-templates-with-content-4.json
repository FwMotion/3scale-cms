{"collection":[{"id":7315494,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Show","system_name":"accounts/invitee_signups/show","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-user\"\u003e\u003c/i\u003e\n        Invitation sign in\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body\"\u003e\n\n        {% include 'login/sso' %}\n\n        {% form 'invitation_signup', user, { class: 'form-horizontal' } %}\n\n          \u003cfieldset\u003e\n\n            {% for field in user.fields %}\n              {% include 'field' with field %}\n            {% endfor %}\n\n            {% if user.password_required? %}\n              \u003cdiv class=\"form-group {{ user.errors.password | error_class: 'has-error' }}\" id=\"user_password_input\"\u003e\n                \u003clabel for=\"user_password\" class=\"control-label col-md-4\"\u003ePassword\u003c/label\u003e\n                \u003cdiv class=\"col-md-6\"\u003e\n                  \u003cinput id=\"user_password\" name=\"user[password]\" tabindex=\"1\"\n                         autofocus=\"autofocus\" type=\"password\" class=\"form-control\"\u003e\n                  {{ user.errors.password | inline_errors: 'inline-errors' }}\n                \u003c/div\u003e\n              \u003c/div\u003e\n\n              \u003cdiv class=\"form-group {{ user.errors.password_confirmation | error_class: 'has-error' }}\" id=\"user_password_confirmation_input\"\u003e\n                \u003clabel for=\"user_password_confirmation\" class=\"control-label col-md-4\"\u003e\n                  Password confirmation\n                \u003c/label\u003e\n                \u003cdiv class=\"col-md-6\"\u003e\n                  \u003cinput id=\"user_password_confirmation\" name=\"user[password_confirmation]\" tabindex=\"2\"\n                         type=\"password\" class=\"form-control\"\u003e\n                  {{ user.errors.password_confirmation | inline_errors: 'inline-errors' }}\n                \u003c/div\u003e\n              \u003c/div\u003e\n            {% endif %}\n\n          \u003c/fieldset\u003e\n\n          {% include 'signup_licence' %}\n\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003cdiv class=\"col-md-10\"\u003e\n                \u003cinput name=\"commit\" type=\"submit\" value=\"Sign up\" class=\"btn btn-success btn-lg pull-right\"\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n        {% endform %}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n"},{"id":7315498,"type":"builtin_page","created_at":"2023-10-27T07:39:37Z","updated_at":"2023-10-27T07:39:37Z","title":"New","system_name":"applications/new","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        New Application\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n        {% form 'application.create', application, class: \"form-horizontal\" %}\n          {% include 'applications/form' %}\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003cdiv class=\"col-md-10\"\u003e\n                \u003cinput class=\"btn btn-primary pull-right\"\n                                 name=\"commit\"\n                                 type=\"submit\"\n                                 value=\"Create Application\" /\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n        {% endform %}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n"},{"id":7315499,"type":"builtin_page","created_at":"2023-10-27T07:39:37Z","updated_at":"2023-10-27T07:39:37Z","title":"Choose service","system_name":"applications/choose_service","layout_id":7315434,"draft":null,"published":"\u003ch2\u003eSelect service\u003c/h2\u003e\n\u003cp\u003eIn order to create an application, select one of the services you are subscribed to.\u003c/p\u003e\n\n\u003cdl class=\"select-service-list\"\u003e\n  {% for service in services %}\n    \u003cdt class=\"service\"\u003e\n      \u003ca href=\"{{ urls.new_application }}?service_id={{ service.system_name }}\" class=\"\"\u003e{{ service.name }}\u003c/a\u003e\n    \u003c/dt\u003e\n    \u003cdd\u003e{{ service.description }}\u003c/dd\u003e\n  {% endfor %}\n\u003c/dl\u003e\n"},{"id":7315500,"type":"builtin_page","created_at":"2023-10-27T07:39:37Z","updated_at":"2023-10-27T07:39:37Z","title":"New","system_name":"services/new","layout_id":7315434,"draft":null,"published":"\u003ch2\u003eSubscribe to Service\u003c/h2\u003e\n\n{% form 'subscription.create' %}\n  \u003cp\u003ePlease choose a service and plan you which to subscribe to.\u003c/p\u003e\n\n  \u003cfieldset class=\"inputs\" name=\"\"\u003e\n    \u003col\u003e\n      \u003cli class=\"plan_selector required\" id=\"service_contract_plan_input\"\u003e\n        \u003clabel for=\"service_contract_plan_id\"\u003ePlan\u003cabbr title=\"required\"\u003e*\u003c/abbr\u003e\u003c/label\u003e\n        \u003cselect id=\"service_contract_plan_id\" name=\"service_contract[plan_id]\"\u003e\n            {% if subscription.service %}\n               \u003coptgroup label=\"{{ subscription.service.name }}\"\u003e\n                 {% for plan in subscription.service.service_plans %}\n                   \u003coption value=\"{{ plan.id }}\"\u003e{{ plan.name }}\u003c/option\u003e\n                 {% endfor %}\n               \u003c/optgroup\u003e\n            {% else %}\n              {% for service in provider.services %}\n                \u003coptgroup label=\"{{ service.name }}\"\u003e\n                  {% for plan in service.service_plans %}\n                    \u003coption value=\"{{ plan.id }}\"\u003e{{ plan.name }}\u003c/option\u003e\n                  {% endfor %}\n                \u003c/optgroup\u003e\n              {% endfor %}\n            {% endif %}\n        \u003c/select\u003e\n        \u003cp class=\"inline-hints\"\u003eChoose a plan you want to subscribe to.\u003c/p\u003e\n      \u003c/li\u003e\n    \u003c/ol\u003e\n  \u003c/fieldset\u003e\n\n  {% include 'service_subscription_licence' %}\n\n  \u003cfieldset class=\"buttons\"\u003e\n    \u003col\u003e\n      \u003cli class=\"commit\"\u003e\n        \u003cinput class=\"important-button create\" name=\"commit\" type=\"submit\" value=\"Subscribe\"\u003e\n      \u003c/li\u003e\n    \u003c/ol\u003e\n  \u003c/fieldset\u003e\n\n{% endform %}\n"},{"id":7315501,"type":"builtin_page","created_at":"2023-10-27T07:39:37Z","updated_at":"2023-10-27T07:39:37Z","title":"Index","system_name":"services/index","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"full\"\u003e\n  \u003cdiv class=\"container\" \u003e\n    \u003cdiv class=\"row\"\u003e\n      \u003cdiv class=\"col-md-10\"\u003e\n        \u003cdiv class=\"panel panel-default\"\u003e\n          \u003cdiv class=\"panel-body\"\u003e\n            \u003ctable class=\"table\"\u003e\n              \u003cthead\u003e\n                \u003ctr\u003e\n                  \u003cth style=\"width:20%\"\u003eName\u003c/th\u003e\n                  \u003cth\u003eDescription\u003c/th\u003e\n                  \u003cth\u003ePlan\u003c/th\u003e\n                  \u003cth\u003e\u003c/th\u003e\n                \u003c/tr\u003e\n              \u003c/thead\u003e\n              \u003ctbody\u003e\n                {% for service in provider.services %}\n                  \u003ctr class=\"service\"\u003e\n                    {% if service.subscribable? %}\n                      {% assign subscription = service.subscription %}\n                      \u003ctd\u003e\n                        {{ service.name }}\n                      \u003c/td\u003e\n                      \u003ctd\u003e\n                        \u003cp\u003e\n                          {{ service.description }}\n                        \u003c/p\u003e\n                      \u003c/td\u003e\n\n                      \u003ctd\u003e\n                        {% if subscription %}\n                          {{ subscription.plan.name  }}\n                          {% unless subscription.live? %}\n                            ({{ subscription.state }})\n                          {% endunless %}\n                        {% endif %}\n                      \u003c/td\u003e\n\n                      \u003ctd\u003e\n                        {% if subscription %}\n                            {% if subscription.can.change_plan? %}\n                               \u003ca href=\"#\" id=\"choose-plan-{{subscription.id}}\" class=\"btn btn-default\"\u003eReview/Change\u003c/a\u003e\n                               {{ \"plans_widget_v2.js\" | javascript_include_tag }}\n                               {{ \"plans_widget.css\" | stylesheet_link_tag }}\n\n                                \u003cscript type=\"text/javascript\"\u003e\n                                  //\u003c![CDATA[\n                                  $(document).ready(function() {\n                                    $(\"#choose-plan-{{ subscription.id }}\").on('click', function(){\n                                      var planID = '{{ subscription.plan.id }}';\n                                      var contractID = '{{ subscription.id }}';\n                                      var url = '{{ subscription.change_plan_url }}';\n\n                                      function plan_chosen_callback(name, planID){\n                                      }\n\n                                      PlanWidget.loadPreview(planID, plan_chosen_callback, url, contractID );\n\n                                      return false;\n                                    });\n                                  });\n                                  //]]\u003e\n                                \u003c/script\u003e\n\n                            {% endif %}\n                       {% else %}\n                         {{ \"Subscribe to \" | append: service.name | link_to: service.subscribe_url }}\n                       {% endif %}\n                      \u003c/td\u003e\n                      {% endif %}\n                  \u003c/tr\u003e\n                 {% endfor %}\n              \u003c/tbody\u003e\n            \u003c/table\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n\n"}],"metadata":{"per_page":20,"total_entries":65,"total_pages":4,"current_page":4}}