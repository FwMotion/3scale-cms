{"collection":[{"id":7315454,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Show","system_name":"dashboards/show","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\" id=\"access-details\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-key\"\u003e\u003c/i\u003e\n        Credentials\n        {% if current_account.applications.size \u003e 0 %}\n          \u003ca class=\"pull-right\" href=\"{{ urls.applications }}\" title=\"inbox\"\u003e\n            \u003ci class=\"fa fa-chevron-right\"\u003e\u003c/i\u003e\n          \u003c/a\u003e\n        {% endif %}\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n        {% if current_account.applications.size == 0 %}\n          \u003ca href=\"{{ urls.new_application }}\" class=\"btn btn-primary\"\u003eCreate Application\u003c/a\u003e\n        {% elsif current_account.applications.size \u003e= 1 %}\n          {% assign app = current_account.applications.first %}\n\n          {% if app.user_key_mode? %}\n            \u003cdl class=\"dl-horizontal\"\u003e\n              \u003cdt\u003eApp name\u003c/dt\u003e\n              \u003cdd\u003e\u003ca href=\"{{ app.url }}\"\u003e{{ app.name }}\u003c/a\u003e\u003c/dd\u003e\n              \u003cdt\u003eKey\u003c/dt\u003e\n              \u003cdd\u003e\n                \u003ccode\u003e{{ app.user_key }}\u003c/code\u003e\n                \u003cp\u003eAdd this as a \u003ccode\u003euser_key\u003c/code\u003e parameter to your API calls to authenticate.\u003c/p\u003e\n              \u003c/dd\u003e\n            \u003c/dl\u003e\n\n          {% elsif app.app_id_mode? %}\n            \u003cdl class=\"dl-horizontal\"\u003e\n              \u003cdt\u003eApp name\u003c/dt\u003e\n              \u003cdd\u003e\u003ca href=\"{{ app.url }}\"\u003e{{ app.name }}\u003c/a\u003e\u003c/dd\u003e\n              \u003cdt\u003eApp ID\u003c/dt\u003e\n              \u003cdd\u003e\u003ccode\u003e{{ app.application_id }}\u003c/code\u003e\u003c/dd\u003e\n              \u003cdt\u003eKey\u003c/dt\u003e\n              \u003cdd\u003e\u003ccode\u003e{{ app.keys.first }}\u003c/code\u003e\u003c/dd\u003e\n            \u003c/dl\u003e\n\n          {% elsif app.oauth_mode? %}\n            \u003cdl class=\"dl-horizontal\"\u003e\n              \u003cdt\u003eApp name\u003c/dt\u003e\n              \u003cdd\u003e\u003ca href=\"{{ app.url }}\"\u003e{{ app.name }}\u003c/a\u003e\u003c/dd\u003e\n              \u003cdt\u003eClient ID\u003c/dt\u003e\n              \u003cdd\u003e\u003ccode\u003e{{ app.application_id }}\u003c/code\u003e\u003c/dd\u003e\n              \u003cdt\u003eClient Secret\u003c/dt\u003e\n              \u003cdd\u003e\u003ccode\u003e{{ app.keys.first }}\u003c/code\u003e\u003c/dd\u003e\n            \u003c/dl\u003e\n          {% endif %}\n\n          {% if app.trial? %}\n            \u003cdiv class=\"alert alert-warning\"\u003e\n              \u003ch3\u003eTrial period\u003c/h3\u003e\n              \u003cp\u003e{{ app.remaining_trial_period_days }} days remaining.\u003c/p\u003e\n            \u003c/div\u003e\n          {% endif %}\n\n        {% else  %}\n          \u003ca href=\"{{ urls.applications }}\" class=\"btn btn-primary\"\u003eList My Application\u003c/a\u003e\n        {% endif %}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n\n\n\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-5\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-inbox\"\u003e\u003c/i\u003e\n        Latest Messages\n        \u003ca class=\"pull-right\" href=\"{{ urls.messages_inbox }}\" title=\"inbox\"\u003e\n          \u003ci class=\"fa fa-chevron-right\"\u003e\u003c/i\u003e\n        \u003c/a\u003e\n\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n        \u003cul class=\"list-unstyled\"\u003e\n          {% if current_account.latest_messages.size \u003e 0 %}\n            {% for message in current_account.latest_messages %}\n              \u003cli\u003e{{ message.subject | link_to: message.url }}\u003c/li\u003e\n            {% endfor %}\n          {% else %}\n            \u003cli\u003eNo messages at this momentâ€¦\u003c/li\u003e\n          {% endif %}\n        \u003c/ul\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\u003c/div\u003e\n"},{"id":7315455,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"New","system_name":"password/new","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-coffee\"\u003e\u003c/i\u003e\n        Forgot password\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body\"\u003e\n        {% form 'password_reset', { class: 'form-horizontal' } %}\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003clabel for=\"email\" class=\"control-label col-md-4\"\u003eEmail address\u003c/label\u003e\n              \u003cdiv class=\"col-md-6\"\u003e\n                \u003cinput id=\"email\" name=\"email\" type=\"text\" class=\"form-control\" autofocus=\"autofocus\"\u003e\n                \u003cspan class=\"help-block\"\u003ePlease enter the email address you used to sign up to this site. Instructions on how to reset your password will be sent to you.\u003c/span\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n          \u003cfieldset\u003e\n            \u003cdiv class=\"col-md-10\"\u003e\n              \u003cinput name=\"commit\" type=\"submit\" class=\"btn btn-success btn-lg pull-right\" value=\"Send instructions\"\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n        {% endform %}\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-footer\"\u003e\n        \u003ca href=\"{{ urls.login }}\" class=\"link\"\u003e\n          \u003ci class=\"fa fa-chevron-left\"\u003e\u003c/i\u003e\n          Sign in\n        \u003c/a\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n"},{"id":7315456,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Show","system_name":"password/show","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"icon-coffee\"\u003e\u003c/i\u003e\n        Change password\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body\"\u003e\n        {% form 'password_change', password_reset_token, { class: 'form-horizontal' } %}\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003clabel for=\"user_password\" class=\"control-label col-md-4\"\u003ePassword\u003c/label\u003e\n              \u003cdiv class=\"col-md-6\"\u003e\n                \u003cinput id=\"user_password\" name=\"user[password]\" type=\"password\" class=\"form-control\" autofocus=\"autofocus\"\u003e\n                 {{ user.errors.password | inline_errors }}\n              \u003c/div\u003e\n\n            \u003c/div\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003clabel for=\"user_password_confirmation\" class=\"control-label col-md-4\"\u003ePassword confirmation\u003c/label\u003e\n              \u003cdiv class=\"col-md-6\"\u003e\n                \u003cinput id=\"user_password_confirmation\" name=\"user[password_confirmation]\" type=\"password\" class=\"form-control\"\u003e\n                 {{ user.errors.password_confirmation | inline_errors }}\n              \u003c/div\u003e\n            \u003c/div\u003e\n\n          \u003c/fieldset\u003e\n          \u003cfieldset\u003e\n            \u003cdiv class=\"col-md-10\"\u003e\n              \u003cinput name=\"commit\" type=\"submit\" class=\"btn btn-success btn-lg pull-right\" value=\"Change Password\"\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n        {% endform %}\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-footer\"\u003e\n        \u003ca href=\"{{ urls.login }}\" class=\"link\"\u003e\n          \u003ci class=\"icon-chevron-left\"\u003e\u003c/i\u003e\n          Sign in\n        \u003c/a\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n"},{"id":7315457,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"New","system_name":"login/new","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-user\"\u003e\u003c/i\u003e\n        Sign in\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body\"\u003e\n        {% include 'login/sso' %}\n        {% form 'login', user, class: 'form-horizontal' %}\n          {% include 'login/cas' with cas %}\n\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\" id=\"session_username_input\"\u003e\n              \u003clabel for=\"session_username\" class=\"control-label col-md-4\"\u003eUsername or Email\u003c/label\u003e\n              \u003cdiv class=\"col-md-6\"\u003e\n                \u003cinput id=\"session_username\" name=\"username\" tabindex=\"1\" autofocus=\"autofocus\"\n                       type=\"text\" placeholder=\"Authenticate with your email\"\n                       class=\"form-control\"\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"form-group\"  id=\"session_password_input\"\u003e\n              \u003clabel for=\"session_password\" class=\"control-label col-md-4\"\u003ePassword\u003c/label\u003e\n              \u003cdiv class=\"col-md-6\"\u003e\n                \u003cinput id=\"session_password\" name=\"password\" tabindex=\"2\"\n                       type=\"password\" placeholder=\"â€¦and password\"\n                       class=\"form-control\"\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n\n            \u003cinput name=\"remember_me\" type=\"hidden\" value=\"1\"\u003e\n          \u003c/fieldset\u003e\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003cdiv class=\"col-md-10\"\u003e\n                \u003cinput name=\"commit\" type=\"submit\" value=\"Sign in\" class=\"btn btn-success btn-lg pull-right\"\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n        {% endform %}\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-footer\"\u003e\n        \u003ca href=\"{{ urls.forgot_password }}\"\u003eForgot password?\u003c/a\u003e\n\n        {% if provider.signups_enabled? %}\n          |  \u003ca href=\"{{ urls.signup }}\" class=\"link\"\u003eSign up\u003c/a\u003e\n        {% endif %}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n"},{"id":7315458,"type":"builtin_partial","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","system_name":"login/sso","draft":null,"published":"{% unless user.oauth2? %}\n  \u003cul class=\"list-auth-provider row list-unstyled\"\u003e\n    {% for auth_provider in site.authentication_providers %}\n        \u003cli class=\"col-md-offset-4 col-md-6\"\u003e\n      \u003ca class=\"btn btn-default auth-provider auth-provider-{{auth_provider.kind}}\" href=\"{{auth_provider.authorize_url}}\"\u003e\n            \u003ci class=\"fa fa-{{auth_provider.kind}}\"\u003e\u003c/i\u003e Authenticate with {{ auth_provider.name }}\n          \u003c/a\u003e\n        \u003c/li\u003e\n    {% endfor %}\n  \u003c/ul\u003e\n{% endunless %}\n"},{"id":7315459,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Show","system_name":"signup/show","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-user\"\u003e\u003c/i\u003e\n        Sign up\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n\n        {% if plans.size \u003e 0 %}\n        \u003cdiv class=\"well well-sm\"\u003e\n          {% if plans.size \u003e 1 %}\n            You are signing up with plans {{ plans | map: 'name' | join:  ', ' }}.\n          {% else %}\n            You are signing up to plan {{ plans.first.name }}.\n          {% endif %}\n        \u003c/div\u003e\n        {% endif %}\n\n        {% include 'signup/cas' with cas %}\n        {% include 'login/sso' %}\n        {% form 'signup', account, class: \"form-horizontal\" %}\n\n          \u003cfieldset\u003e\n            {% if account.errors.plans == present %}\n              \u003cdiv class=\"alert alert-danger\"\u003e{{ account.errors.plans | inline_errors: 'inline-errors' }}\u003c/div\u003e\n            {% endif %}\n\n            {% for field in account.fields %}\n              {% include 'field' with field %}\n            {% endfor %}\n          \u003c/fieldset\u003e\n\n          \u003cfieldset\u003e\n            {% for field in user.fields %}\n              {% include 'field' with field %}\n            {% endfor %}\n          \u003c/fieldset\u003e\n\n          {% if user.password_required? %}\n            \u003cfieldset\u003e\n              \u003cdiv class=\"form-group {{ user.errors.password | error_class: 'has-error' }}\"\u003e\n                \u003clabel for=\"account_user_password\" class=\"control-label col-md-4\"\u003e\n                  Password\n                  \u003cabbr class=\"required\" title=\"required\"\u003e*\u003c/abbr\u003e\n                \u003c/label\u003e\n                \u003cdiv class=\"col-md-6\"\u003e\n                  \u003cinput id=\"account_user_password\"\n                         name=\"account[user][password]\" type=\"password\"\n                         class=\"form-control\"\u003e\n                  {{ user.errors.password | inline_errors: 'inline-errors' }}\n                \u003c/div\u003e\n              \u003c/div\u003e\n\n              \u003cdiv class=\"form-group {{ user.errors.password_confirmation | error_class: 'has-error' }}\"\u003e\n                \u003clabel for=\"account_user_password_confirmation\" class=\"control-label col-md-4\"\u003e\n                  Password confirmation\n                  \u003cabbr class=\"required\" title=\"required\"\u003e*\u003c/abbr\u003e\n                \u003c/label\u003e\n                \u003cdiv class=\"col-md-6\"\u003e\n                  \u003cinput id=\"account_user_password_confirmation\"\n                         name=\"account[user][password_confirmation]\"\n                         type=\"password\"\n                         class=\"form-control\" \u003e\n                  {{ user.errors.password_confirmation | inline_errors: 'inline-errors' }}\n                \u003c/div\u003e\n              \u003c/div\u003e\n            \u003c/fieldset\u003e\n          {% endif %}\n\n\n          {% comment %}\n            The 'signup_licence' partial is usually a place where a legal text\n            binding the developer on signup sits. It it does not exist\n            [default], the include is gracefully ignored.\n          {% endcomment %}\n\n\n          {% include 'signup_licence' %}\n\n          \u003cdiv class=\"form-group\"\u003e\n            \u003cdiv class=\"col-md-10\"\u003e\n              \u003cfieldset\u003e\n                \u003cinput name=\"commit\" type=\"submit\" class=\"btn btn-success btn-lg pull-right\" value=\"Sign up\"/\u003e\n              \u003c/fieldset\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cinput type=\"hidden\" id=\"captchaChecked\" name=\"captchaChecked\" value=\"\"\u003e\n        {% endform %}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n{{ 'validate_signup.js' | javascript_include_tag }}\n\u003cscript type=\"text/javascript\"\u003e\n  document.querySelector('input:not([type=\"hidden\"])').focus()\n\n  const inputCaptcha = document.querySelector('#captchaChecked')\n  function onCaptchaSuccess () {\n    inputCaptcha.value = 'ok'\n    inputCaptcha.dispatchEvent(new Event('keyup'))\n  }\n  function onCaptchaFail () {\n    inputCaptcha.value = ''\n    inputCaptcha.dispatchEvent(new Event('keyup'))\n  }\n\u003c/script\u003e\n"},{"id":7315460,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Success","system_name":"signup/success","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ci class=\"fa fa-user\"\u003e\u003c/i\u003e\n        Sign up\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n        \u003ch2\u003eThank you\u003c/h2\u003e\n        \u003cp class=\"lead\"\u003eWe have sent you an email to confirm your email address. Please click the link in the email and you can directly login!\u003c/p\u003e\n        \u003cp\u003e(If you don't see the email, please check your spam box as well.)\u003c/p\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e"},{"id":7315461,"type":"builtin_partial","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","system_name":"signup/cas","draft":null,"published":"{