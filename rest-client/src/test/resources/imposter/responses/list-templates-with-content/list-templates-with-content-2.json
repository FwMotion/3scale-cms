 %}\n            \u003ctr class=\"{% cycle 'messages': 'odd', 'even' %}\"\u003e\n              \u003ctd\u003e{{ message.subject | link_to: message.url }}\u003c/td\u003e\n              \u003ctd\u003e{{ message.sender | link_to: message.url }}\u003c/td\u003e\n              \u003ctd\u003e{{ message.created_at }}\u003c/td\u003e\n              \u003ctd\u003e{{ 'Restore' | delete_button: message.url, class: 'btn btn-default' }}\u003c/td\u003e\n            \u003c/tr\u003e\n          {% endfor %}\n        {% else %}\n          \u003ctr\u003e\n            \u003ctd class=\"empty\" colspan=\"4\"\u003eYour trash is empty.\u003c/td\u003e\n          \u003c/tr\u003e\n        {% endif %}\n      \u003c/tbody\u003e\n      \u003ctfoot class=\"panel-footer\"\u003e\n        \u003ctr\u003e\n          \u003ctd colspan=\"4\"\u003e\n            {% if messages.size \u003e 0  %}\n              {{ \"Empty the trash\" | delete_button: urls.empty_messages_trash, class: 'btn btn-danger' }}\n            {% endif %}\n          \u003c/td\u003e\n        \u003c/tr\u003e\n      \u003c/tfoot\u003e\n    \u003c/table\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n{% include 'shared/pagination' with pagination %}\n"},{"id":7315469,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Show","system_name":"messages/trash/show","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    {% include 'messages/menu' %}\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading important\"\u003e\n        {{ message.subject }} \u003cspan class=\"pull-right\"\u003eby {{ message.sender }} at {{ message.created_at | date: i18n.long_date }}\u003c/span\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n        \u003cpre class=\"message\"\u003e{{ message.body }}\u003c/pre\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e"},{"id":7315470,"type":"builtin_partial","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","system_name":"stats/chart","draft":null,"published":"{% content_for stylesheets %}\n  {{ 'stats.css' | stylesheet_link_tag }}\n  {% cdn_asset /jquery-ui/1.11.4/jquery-ui.css %}\n{% endcontent_for %}\n\n{% content_for javascripts %}\n    {{ 'stats.js' | javascript_include_tag }}\n{% endcontent_for %}\n\n\u003cdiv class=\"panel panel-default\"\u003e\n  \u003cdiv class=\"panel-heading\"\u003e\n    \u003ci class=\"fa fa-bar-chart\"\u003e\u003c/i\u003e\n    Graphs\n  {% if provider.multiple_applications_allowed? %}\n      \u003cdiv id=\"applications-selector-container\" class=\"StatsApplicationsSelectorContainer\"\u003e\n          \u003cselect id=\"application-select\" name=\"application\"\u003e\n              {% assign groups = applications | group_by: 'service' %}\n              {% for group in groups %}\n                  \u003coptgroup label=\"{{group[0].name}}\"\u003e\n                      {% for application in group[1] %}\n                          \u003coption value=\"{{application.id}}\"\u003e{{ application.name }}\u003c/option\u003e\n                      {% endfor %}\n                  \u003c/optgroup\u003e\n              {% endfor %}\n          \u003c/select\u003e\n      \u003c/div\u003e\n  {% endif %}\n  \u003c/div\u003e\n  \u003cdiv class=\"panel-body\"\u003e\n    \u003cdiv class=\"StatsMenuContainer\" id=\"menu-container\"\u003e\u003c/div\u003e\n    \u003cdiv class=\"StatsSelectorContainer\" id=\"selector-container\"\u003e\u003c/div\u003e\n    \u003cdiv class=\"StatsChartContainer\" id=\"chart-container\"\u003e\u003c/div\u003e\n    \u003cp id=\"timezone-info\" data-timezone-identifier=\"{{provider.timezone.identifier}}\" class=\"Stats-message--notice\"\u003e\n        Using time zone {{ provider.timezone.full_name }}\n    \u003c/p\u003e\n    \u003cdiv class=\"StatsMethodsTableContainer\" id=\"methods-table-container\"\u003e\u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"panel-footer\"\u003e\n    \u003cdiv class=\"CsvLinkContainer\" id=\"csv-link-container\"\u003e\u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n\n\u003cscript type=\"text/javascript\" charset=\"utf-8\"\u003e\n  (function() {\n    var applicationGroups = {};\n\n    {% if provider.multiple_applications_allowed? %}\n      /*\n        Gets the service name and application names and ids from the liquid rendered selector\n        Returns an object with the service name as a key, and an array of applications\n        I.E: {'Echo API': [{id: 42, name: 'The Answer'}]}\n       */\n      function applicationGroupsFromSelector () {\n        $('.StatsApplicationsSelectorContainer select optgroup').each(function (_i, group) {\n          let applications = applicationGroups[$(group).attr('label')] = [];\n          $(group).children().each(function (_j, application) {\n            applications.push({id: $(application).val(), name: $(application).html()})\n          })\n        })\n        return applicationGroups;\n      }\n      applicationGroups = applicationGroupsFromSelector()\n    {% endif %}\n\n    Stats.statsApplication(jQuery('#client-name').attr('data-client'),\n      {\n        menuContainer: '#menu-container',\n        selectorContainer: '#selector-container',\n        chartContainer: '#chart-container',\n        methodsTableContainer: '#methods-table-container',\n        csvLinkContainer: '#csv-link-container',\n        timezone: $('#timezone-info').data('timezone-identifier'),\n        hasApplicationsSelector: Object.keys(applicationGroups).length \u003e 0,\n        applicationGroups: applicationGroups,\n        applicationsSelectorContainer: '#applications-selector-container'\n      }\n    );\n\n    })();\n\u003c/script\u003e\n"},{"id":7315471,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Index","system_name":"stats/index","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  {% if provider.multiple_applications_allowed? %}\n    {% if applications == present %}\n      \u003cdiv id=\"client-name\" data-client=\"{{ applications.first.id }}\" style=\"display:none\"\u003e\u003c/div\u003e\n      {% include 'stats/chart' %}\n    {% else %}\n      \u003cp\u003eYou do not currently have any live applications.\u003c/p\u003e\n    {% endif %}\n  {% else %}\n    \u003cdiv id=\"client-name\" data-client=\"{{ applications.first.id }}\" style=\"display:none\"\u003e\u003c/div\u003e\n    {% include 'stats/chart' %}\n  {% endif %}\n\u003c/div\u003e\n"},{"id":7315472,"type":"builtin_partial","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","system_name":"applications/form","draft":null,"published":"\u003cinput type=\"hidden\" id=\"cinstance_plan_id\" name=\"application[plan_id]\" value=\"{{ application.plan.id }}\" /\u003e\n\n\u003cfieldset\u003e\n  \u003cdiv class=\"form-group\" id=\"selected-plan\" data-plan-id=\"{{ application.plan.id }}\"\u003e\n    \u003clabel class=\"control-label col-md-4\"\u003ePlan\u003c/label\u003e\n    \u003cdiv class=\"col-md-6\"\u003e\n\n        \u003cp class=\"form-control-static\"\u003e\n          \u003cstrong id='selected-plan-name'\u003e{{application.plan.name}}\u003c/strong\u003e\n          {% if application.can_change_plan? %}\n            \u003cspan\u003eâ€º \u003c/span\u003e\n            \u003ca href=\"#choose-plan-{{ application.id }}\" id=\"choose-plan-{{application.id}}\"\u003e\n              Review/Change\n            \u003c/a\u003e\n            {% plan_widget application, wizard: true %}\n            {{ '/css/plans_widget_overrides.css' | stylesheet_link_tag }}\n          {% endif %}\n        \u003c/p\u003e\n\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/fieldset\u003e\n\n\u003cfieldset\u003e\n  {% for field in application.fields %}\n    {% include 'field' with field %}\n  {% endfor %}\n\u003c/fieldset\u003e\n\n{% include 'new_application_licence' %}\n\n"},{"id":7315473,"type":"builtin_page","created_at":"2023-10-26T15:38:53Z","updated_at":"2023-10-26T15:38:53Z","title":"Edit","system_name":"applications/edit","layout_id":7315434,"draft":null,"published":"\u003cdiv class=\"row\"\u003e\n  \u003cdiv class=\"col-md-9\"\u003e\n    \u003cdiv class=\"panel panel-default\"\u003e\n      \u003cdiv class=\"panel-heading\"\u003e\n        \u003ca href=\"{{ application.url }}\"\u003e\n          \u003ci class=\"fa fa-chevron-left\"\u003e\u003c/i\u003e\n          {{ application.name }}\n        \u003c/a\u003e\n        {% if application.can.be_destroyed? %}\n          \u003ca href=\"{{ application.path }}\" class=\"pull-right\" data-method=\"delete\" data-confirm=\"Are you sure you want to delete the application {{ application.name }} ?\"\u003e\n            \u003ci class=\"fa fa-trash-o\"\u003e\u003c/i\u003e\n            Delete {{ application.name }}\n          \u003c/a\u003e\n        {% endif %}\n      \u003c/div\u003e\n      \u003cdiv class=\"panel-body panel-footer\"\u003e\n        {% form 'application.update', application, class: \"form-horizontal\" %}\n          {% include 'applications/form' %}\n          \u003cfieldset\u003e\n            \u003cdiv class=\"form-group\"\u003e\n              \u003cdiv class=\"col-md-10\"\u003e\n                \u003cinput class=\"btn btn-primary pull-right\"\n                  name=\"commit\"\n                  type=\"submit\"\n                  value=\"Update Application\" /\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/fieldset\u003e\n        {% endform %}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n"}],"metadata":{"per_page":20,"total_entries":65,"total_pages":4,"current_page":2}}